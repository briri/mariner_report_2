<div class="admin-form">
  <h2 class="article-title">
    Editing - <%= @publisher.name %>
  </h2>
  
  <%= form_for @publisher, 
               url: admin_publisher_path(@publisher.slug), 
               html: {multipart: true, method: :put} do |f| %>
    
    <%= render partial: 'form', locals: {f: f} %>
  <% end %>

  <a href="<%= new_admin_publisher_feed_path(@publisher.slug) %>" class="admin-links">
    Add a New Feed
  </a>

  <div class="tabular-association">
    <%= render partial: 'admin/feeds/index', 
               locals: {feeds: @publisher.feeds} %>
  </div>
</div>